{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "bind",
      "camelcase": "Bind",
      "title": "Bind",
      "scope": "source.bind",
      "file-types": ["bind"],
      "injection-regex": "^bind$",
      "class-name": "TreeSitterBind"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Bind grammar for tree-sitter",
    "authors": [
      {
        "name": "Alex Suraci",
        "email": "suraci.alex@gmail.com",
        "url": "https://github.com/vito"
      }
    ],
    "links": {
      "repository": "https://github.com/vito/bind"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
