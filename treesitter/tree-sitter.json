{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "sprout",
      "camelcase": "Sprout",
      "title": "Sprout",
      "scope": "source.sprout",
      "file-types": ["sprout"],
      "injection-regex": "^sprout$",
      "class-name": "TreeSitterSprout"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Sprout grammar for tree-sitter",
    "authors": [
      {
        "name": "Alex Suraci",
        "email": "suraci.alex@gmail.com",
        "url": "https://github.com/vito"
      }
    ],
    "links": {
      "repository": "https://github.com/vito/sprout"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
