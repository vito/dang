# Test 1: Does the fix work when calling methods from outside the module?

type Counter {
	let count = 0

	pub increment: Counter! {
		self.count = self.count + 1
		self
	}
}

# Calling from outside - this should still work because we're using self.
let c1 = Counter.increment
assert { c1.count == 1 }

let c2 = Counter.increment
assert { c2.count == 1 }
