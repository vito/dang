# Test list.filter() method

# Basic functionality - filter even numbers
pub numbers: [Int!]! = [1, 2, 3, 4, 5]
pub evens: [Int!]! = numbers.filter { x -> x % 2 == 0 }
assert { evens[0] == 2 }
assert { evens[1] == 4 }

# Filter strings that start with 'h'
pub words: [String!]! = ["hello", "world", "hi", "there"]
pub h_words: [String!]! = words.filter { w -> w.hasPrefix("h") }
assert { h_words[0] == "hello" }
assert { h_words[1] == "hi" }

# Filter all elements (none match)
pub all_filtered: [Int!]! = [1, 2, 3].filter { x -> false }
assert { all_filtered == [] }

# Filter no elements (all match)
pub none_filtered: [Int!]! = [1, 2, 3].filter { x -> true }
assert { none_filtered == [1, 2, 3] }

# Empty list
pub empty: [Int!]! = []
pub filtered_empty: [Int!]! = empty.filter { x -> x > 0 }
assert { filtered_empty == [] }

print("List filter tests passed!")
