# Test basic case expressions

# Simple literal matching
pub result1 = case (1) {
	1 => "one"
	2 => "two"
	0 => "other"
}

assert { result1 == "one" }

# Multiple values tested
pub result2 = case (2) {
	1 => "one"
	2 => "two"
	3 => "three"
}

assert { result2 == "two" }

# String matching
pub result3 = case ("hello") {
	"world" => "not this"
	"hello" => "matched"
}

assert { result3 == "matched" }

print("Case expression tests passed!")
