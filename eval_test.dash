# Test evaluation engine with working constructs

# Simple values
pub str = "hello world"
pub bool_true = true
pub bool_false = false

# Let binding
pub let_test = let x = "test" in x

# Conditional expressions
pub cond_true = if true { "yes" } else { "no" }
pub cond_false = if false { "yes" } else { "no" }

# Lambda expressions
pub identity = \x -> x